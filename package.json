{
  "name": "multi-cursor-ai-generate",
  "displayName": "%displayName%",
  "description": "%description%",
  "publisher": "CHENJIAMIAN",
  "author": "CHENJIAMIAN",
  "version": "0.2.3",
  "engines": {
    "vscode": "^1.92.0"
  },
  "l10n": "./l10n",
  "main": "./dist/src/extension.js",
  "categories": [
    "Other"
  ],
  "icon": "images/icon.png",
  "keywords": [
    "ai",
    "openai",
    "multi-cursor",
    "multi select",
    "generate",
    "chatgpt",
    "sse",
    "rate limit",
    "undici"
  ],
  "homepage": "https://github.com/CHENJIAMIAN/vscode-extension-multi-cursor-ai-generate#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/CHENJIAMIAN/vscode-extension-multi-cursor-ai-generate"
  },
  "bugs": {
    "url": "https://github.com/CHENJIAMIAN/vscode-extension-multi-cursor-ai-generate/issues"
  },
  "license": "MIT",
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "multiCursorAI.generate",
        "title": "%command.generate.title%",
        "category": "%configuration.title%"
      },
      {
        "command": "multiCursorAI.syncModels",
        "title": "%command.syncModels.title%",
        "category": "%configuration.title%"
      },
      {
        "command": "multiCursorAI.setApiKey",
        "title": "%command.setApiKey.title%",
        "category": "%configuration.title%"
      },
      {
        "command": "multiCursorAI.clearApiKey",
        "title": "%command.clearApiKey.title%",
        "category": "%configuration.title%"
      },
      {
        "command": "multiCursorAI.toggleUseStreaming",
        "title": "%command.toggleUseStreaming.title%",
        "category": "%configuration.title%"
      },
      {
        "command": "multiCursorAI.setTemperature",
        "title": "%command.setTemperature.title%",
        "category": "%configuration.title%"
      },
      {
        "command": "multiCursorAI.setPreSeparator",
        "title": "%command.setPreSeparator.title%",
        "category": "%configuration.title%"
      },
      {
        "command": "multiCursorAI.toggleTrimResult",
        "title": "%command.toggleTrimResult.title%",
        "category": "%configuration.title%"
      },
      {
        "command": "multiCursorAI.setLogLevel",
        "title": "%command.setLogLevel.title%",
        "category": "%configuration.title%"
      },
      {
        "command": "multiCursorAI.setReasoningEffort",
        "title": "%command.setReasoningEffort.title%",
        "category": "%configuration.title%"
      },
      {
        "command": "multiCursorAI.toggleDisableReasoning",
        "title": "%command.toggleDisableReasoning.title%",
        "category": "%configuration.title%"
      },
      {
        "command": "multiCursorAI.toggleSingleLineOutput",
        "title": "%command.toggleSingleLineOutput.title%",
        "category": "%configuration.title%"
      }
    ],
    "keybindings": [
      {
        "command": "multiCursorAI.generate",
        "key": "ctrl+alt+p",
        "when": "editorTextFocus && !inputFocus",
        "linux": "ctrl+alt+p",
        "win": "ctrl+alt+p"
      },
      {
        "command": "multiCursorAI.generate",
        "key": "ctrl+option+p",
        "when": "editorTextFocus && !inputFocus",
        "mac": "ctrl+option+p"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "multiCursorAI.generate",
          "group": "navigation@10"
        },
        {
          "command": "multiCursorAI.syncModels",
          "group": "navigation@11"
        },
        {
          "command": "multiCursorAI.setApiKey",
          "group": "navigation@90"
        },
        {
          "command": "multiCursorAI.clearApiKey",
          "group": "navigation@91"
        }
      ]
    },
    "configuration": {
      "title": "%configuration.title%",
      "properties": {
        "multiCursorAI.baseUrl": {
          "type": "string",
          "default": "https://api.openai.com",
          "description": "%config.baseUrl.description%"
        },
        "multiCursorAI.apiKeySecretId": {
          "type": "string",
          "default": "multiCursorAI.apiKey",
          "description": "%config.apiKeySecretId.description%"
        },
        "multiCursorAI.apiKeyEnvVar": {
          "type": "string",
          "default": "OPENAI_API_KEY",
          "description": "%config.apiKeyEnvVar.description%"
        },
        "multiCursorAI.authScheme": {
          "type": "string",
          "default": "Bearer",
          "description": "%config.authScheme.description%"
        },
        "multiCursorAI.modelDefault": {
          "type": "string",
          "default": "gpt-4o-mini",
          "description": "%config.modelDefault.description%"
        },
        "multiCursorAI.modelList": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            "gpt-4o-mini",
            "gpt-4o",
            "gpt-4.1-mini",
            "gpt-3.5-turbo"
          ],
          "description": "%config.modelList.description%"
        },
        "multiCursorAI.modelsPath": {
          "type": "string",
          "default": "/v1/models",
          "description": "%config.modelsPath.description%"
        },
        "multiCursorAI.requestPath": {
          "type": "string",
          "default": "/v1/chat/completions",
          "description": "%config.requestPath.description%"
        },
        "multiCursorAI.useStreaming": {
          "type": "boolean",
          "default": true,
          "description": "%config.useStreaming.description%"
        },
        "multiCursorAI.timeoutMs": {
          "type": "number",
          "default": 60000,
          "description": "%config.timeoutMs.description%"
        },
        "multiCursorAI.proxy": {
          "type": "string",
          "default": "",
          "markdownDescription": "%config.proxy.description%"
        },
        "multiCursorAI.rejectUnauthorized": {
          "type": "boolean",
          "default": true,
          "description": "%config.rejectUnauthorized.description%"
        },
        "multiCursorAI.temperature": {
          "type": "number",
          "default": 0.2,
          "minimum": 0,
          "maximum": 2,
          "description": "%config.temperature.description%"
        },
        "multiCursorAI.maxTokens": {
          "type": "number",
          "default": 0,
          "minimum": 0,
          "description": "%config.maxTokens.description%"
        },
        "multiCursorAI.requestBodyMode": {
          "type": "string",
          "enum": [
            "auto",
            "chat",
            "completions"
          ],
          "default": "auto",
          "description": "%config.requestBodyMode.description%"
        },
        "multiCursorAI.maxConcurrency": {
          "type": "number",
          "default": 30,
          "minimum": 1,
          "description": "%config.maxConcurrency.description%"
        },
        "multiCursorAI.httpMaxConnections": {
          "type": "number",
          "default": 30,
          "minimum": 1,
          "description": "%config.httpMaxConnections.description%"
        },
        "multiCursorAI.maxRequestsPerMinute": {
          "type": "number",
          "default": 60,
          "minimum": 1,
          "description": "%config.maxRequestsPerMinute.description%"
        },
        "multiCursorAI.dynamicLimitProbe": {
          "type": "boolean",
          "default": true,
          "description": "%config.dynamicLimitProbe.description%"
        },
        "multiCursorAI.maxRetries": {
          "type": "number",
          "default": 8,
          "minimum": 0,
          "description": "%config.maxRetries.description%"
        },
        "multiCursorAI.baseBackoffMs": {
          "type": "number",
          "default": 500,
          "minimum": 100,
          "description": "%config.baseBackoffMs.description%"
        },
        "multiCursorAI.maxBackoffMs": {
          "type": "number",
          "default": 60000,
          "minimum": 1000,
          "description": "%config.maxBackoffMs.description%"
        },
        "multiCursorAI.insertMode": {
          "type": "string",
          "enum": [
            "append",
            "replace"
          ],
          "default": "append",
          "description": "%config.insertMode.description%"
        },
        "multiCursorAI.preSeparator": {
          "type": "string",
          "default": " ",
          "description": "%config.preSeparator.description%"
        },
        "multiCursorAI.postSeparator": {
          "type": "string",
          "default": "",
          "description": "%config.postSeparator.description%"
        },
        "multiCursorAI.trimResult": {
          "type": "boolean",
          "default": true,
          "description": "%config.trimResult.description%"
        },
        "multiCursorAI.promptTemplate": {
          "type": "string",
          "default": "请根据用户意图对所选文本进行处理。\\n用户意图: {userPrompt}\\n所选文本: \\n\\n{selection}",
          "markdownDescription": "%config.promptTemplate.description%"
        },
        "multiCursorAI.systemPromptEnabled": {
          "type": "boolean",
          "default": false,
          "description": "%config.systemPromptEnabled.description%"
        },
        "multiCursorAI.globalPrependInstruction": {
          "type": "string",
          "default": "",
          "markdownDescription": "%config.globalPrependInstruction.description%"
        },
        "multiCursorAI.contextVars.includeFileName": {
          "type": "boolean",
          "default": true,
          "description": "%config.contextVars.includeFileName.description%"
        },
        "multiCursorAI.contextVars.includeLanguageId": {
          "type": "boolean",
          "default": true,
          "description": "%config.contextVars.includeLanguageId.description%"
        },
        "multiCursorAI.contextVars.includeRelativePath": {
          "type": "boolean",
          "default": true,
          "description": "%config.contextVars.includeRelativePath.description%"
        },
        "multiCursorAI.contextVars.includeNow": {
          "type": "boolean",
          "default": true,
          "description": "%config.contextVars.includeNow.description%"
        },
        "multiCursorAI.logLevel": {
          "type": "string",
          "enum": [
            "error",
            "warn",
            "info",
            "debug",
            "trace"
          ],
          "default": "info",
          "description": "%config.logLevel.description%"
        },
        "multiCursorAI.promptHistoryLimit": {
          "type": "number",
          "default": 100,
          "minimum": 1,
          "maximum": 1000,
          "description": "%config.promptHistoryLimit.description%"
        },
        "multiCursorAI.reasoningEffort": {
          "type": "string",
          "enum": [
            "none",
            "low",
            "medium",
            "high"
          ],
          "default": "medium",
          "markdownDescription": "%config.reasoningEffort.description%"
        },
        "multiCursorAI.disableReasoning": {
          "type": "boolean",
          "default": false,
          "markdownDescription": "%config.disableReasoning.description%"
        },
        "multiCursorAI.singleLineOutput": {
          "type": "boolean",
          "default": false,
          "description": "%config.singleLineOutput.description%"
        }
      }
    }
  },
  "scripts": {
    "compile": "tsc -p .",
    "watch": "tsc -w -p .",
    "lint": "eslint . --ext .ts",
    "test": "mocha -r ts-node/register \"test/**/*.test.ts\"",
    "package": "vsce package",
    "publish": "vsce publish --yarn",
    "prepublishOnly": "npm run compile"
  },
  "devDependencies": {
    "@types/chai": "^5.2.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "^18.19.59",
    "@types/vscode": "^1.92.0",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "chai": "^4.5.0",
    "eslint": "^8.57.0",
    "mocha": "^10.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "vsce": "^2.15.0"
  },
  "dependencies": {
    "undici": "^6.19.7"
  }
}